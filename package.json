{
  "name": "@otoshimtoshi/wrap-element-ui",
  "description": "Wrap Element UI",
  "version": "0.1.7",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:lib": "webpack --config ./build/webpack.config.js && webpack --config ./build/webpack.components.js",
    "build:gulp": "gulp build",
    "build:all": "npm run build:gulp && npm run build:lib",
    "lint": "vue-cli-service lint"
  },
  "author": "Masatoshi Otoshi",
  "keywords": [
    "vue",
    "element-ui"
  ],
  "license": "MIT",
  "main": "lib/wrap-element-ui.js",
  "files": [
    "lib",
    "src",
    "vetur"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/otoshimtoshi/wrap-element-ui.git"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@nuxtjs/composition-api": "^0.22.4",
    "core-js": "^3.9.1",
    "element-ui": "^2.15.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-service": "~4.5.12",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.0.5",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "~4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  }
}
